<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cristhian.apptivities">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Application.App"
        android:allowBackup="true"
        android:icon="@drawable/apptivities"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitites.MainActivity" />
        <activity android:name=".Activitites.ActividadActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitites.CategoriaActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activitites.ChartActivity" android:screenOrientation="portrait"/>

    </application>

</manifest>